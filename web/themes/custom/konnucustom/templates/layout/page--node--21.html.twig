        <span class="line line-1"></span>
        <span class="line line-2"></span>
        <span class="line line-3"></span>
      </div>
    </div></div></section>
    {{ page.header }}
  </div>
  </header>
  {{ page.highlighted }}
  {{ page.help }}
  </div>
  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}
    <div class="layout-content">
        <div class="layout-content__inner">
          <h2>{{ drupal_title() }}</h2>
          <div class="contact__inner">
          {% if node.field_header_afbeelding|length %}
          <div class="contact__header">
            <img src="{{ file_url(node.field_header_afbeelding[0].entity.uri.value|image_style('header_contact')) }}" />
          </div>
          <div class="mapouter"><div class="gmap_canvas"><iframe width="672" height="418" id="gmap_canvas" src="https://maps.google.com/maps?q=fosfari&t=&z=13&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe><a href="https://www.whatismyip-address.com/nordvpn-coupon/"></a></div><style>.mapouter{position:relative;text-align:right;height:418px;width:672px;}.gmap_canvas {overflow:hidden;background:none!important;height:418px;width:672px;}</style></div>
        {% endif %}
      </div>
           {{ page.content }}
        </div>
    </div>{# /.layout-content #}

    {% if page.sidebar_first %}
      <aside class="layout-sidebar-first" role="complementary">
        {{ page.sidebar_first }}
      </aside>
    {% endif %}
    {% if page.content_second %}
      <section class="content-second" role="complementary">
        {{ page.content_second }}
      </section>
    {% endif %}

    {% if page.sidebar_second %}
      <aside class="layout-sidebar-second" role="complementary">
        {{ page.sidebar_second }}
      </aside>
    {% endif %}
     {% if page.content_third %}
      <section class="content-third" role="complementary">
        {{ page.content_third }}
      </section>
    {% endif %}

  </main>
     <footer role="contentinfo">

      <div class="footer__inner">

      {% if page.footer %}
        {{ page.footer }}
      {% endif %}
         <div class="footer__address">
        <div class="footer__right"><div class="footer__right--left">{{ 'Leuvensesteenweg 136, 3191 Boortmeerbeek, Belgium'|t }}</div>
          <div class="footer__right--right">
            <div class="footer__right--contact"><a href="tel:+3215518534">015/518534</a><a href="mailto:info@fosfari.be">info@fosfari.be</a></div>
            <div class="footer__right--social">
              <a href="https://www.facebook.com/Fosfari"><i class="fi-xnsuxl-facebook"></i></a>
              <a href="https://www.instagram.com/fosfari_lighting/"><i class="fi-xnsuxl-instagram"></i></a>
              <a href="https://www.linkedin.com/company/fosfari/"><i class="fi-snsuxl-linkedin"></i></a>
            </div>
          </div>
        </div>
      </div>
      <div class="footer__about">
        <div class="footer__about--rights"><div>&copy; {{ "now"|date("Y") }} Fosfari.</div>
        <div class="footer__about--right">
          {{ drupal_menu('footer') }}
          <div class="footer__website">Website by <a href="https://www.konnu.be" alt="Website by Konnu">Konnu</a>.</div></div>
        </div>
      </div>

    </div>
    </footer>

